resource/name = "inicio"
script/source = "extends Node\n\nvar screen_size\nvar window_size\n\nfunc _ready():\n\t# Called every time the node is added to the scene.\n\t# Initialization here\n\tscreen_size = OS.get_screen_size()\n\twindow_size = OS.get_window_size()\n\tOS.set_window_position(screen_size*0.5 - window_size*0.5)\n\t\n\tgame.alignGlobalCenter(self.get_node(\"lbl_titulo\"))\n\tgame.alignGlobalCenter(self.get_node(\"btn_start_game\"))\n\tgame.alignGlobalCenter(self.get_node(\"btn_scores\"))\n\tgame.alignGlobalCenter(self.get_node(\"btn_exit\"))\n\t\n\tpass\n\nfunc _on_start_game_button_down():\n\tself.queue_free()\n\tvar current_scene\n\tvar s = ResourceLoader.load(\"res://scenes/main.tscn\")\n\tcurrent_scene = s.instance()\n\tget_tree().get_root().add_child(current_scene)\n\tget_tree().set_current_scene( current_scene )\n\tpass # replace with function body\n\n\nfunc _on_btn_exit_button_down():\n\tgame.sair()\n\tpass # replace with function body\n"

