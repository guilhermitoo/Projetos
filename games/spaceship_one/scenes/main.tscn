func _ready():
	# ATRIBUI A VARIAVEL DE VIDA INICIAL PARA A VARIAVEL VIDA
	get_node(\"lbl_vidas\").set_text(\"x \" + str(game.nave_hp))
	game.connect(\"lifes_changed\" , self , \"_on_lifes_changed\")
	pass

func _on_lifes_changed():
	update()
	if game.nave_vida <= 0:
		game.fimDeJogo()
	pass
	
func update():
	get_node(\"lbl_vidas\").set_text(\"x \" + str(game.nave_vida))"

