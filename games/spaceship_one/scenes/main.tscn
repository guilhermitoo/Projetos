[sub_resource type="GDScript" id=5]

script/source = "extends Node2D

# ADICIONA A PROPRIEDADE TEXTURA NO NODE2D, PODENDO
# SER MANIPULADA PELO INSPECTOR
export(Texture) var textura

func _ready():
	game.connect(\"lifes_changed\" , self , \"_on_lifes_changed\")
	pass

func _on_lifes_changed():
	update()
	if game.nave_vida <= 0:
		game.fimDeJogo()
	pass
	
func _draw():
	for n in range(0, game.nave_vida):
		draw_texture(textura,Vector2(n * textura.get_width() , 0) )
	pass"

